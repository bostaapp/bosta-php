{
    "name": "bosta/bosta-sdk",
    "description": "A PHP client SDK for Bosta APIs integration.",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "bosta",
        "bosta-sdk"
    ],
    "homepage": "https://github.com/bostaapp/bosta-php",
    "license": "MIT",
    "authors": [
        {
            "name": "aliaasayed",
            "email": "aliaasayed45@gmail.com",
            "homepage": "https://github.com/aliaasayed",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.11",
        "squizlabs/php_codesniffer": "^3.5.6"
    },
    "autoload": {
        "psr-4": {
            "Bosta\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}